@page
@using Microsoft.AspNetCore.Mvc.Localization
@using ToksozBysNew.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using ToksozBysNew.Web.Pages.AccountGroups
@using ToksozBysNew.AccountGroups;
@using System.Globalization
@inject IHtmlLocalizer<ToksozBysNewResource> L
@model CreateModalModel
@{
    Layout = null;
}

<form data-ajaxForm="true" asp-page="/AccountGroups/CreateModal" autocomplete="off">
    <abp-modal id="AccountGroupCreateModal">
        <abp-modal-header title="@L["NewAccountGroup"].Value"></abp-modal-header>

        <abp-modal-body>
                    
            <abp-input asp-for="AccountGroup.AccountGroupName" />

            <abp-input asp-for="AccountGroup.IsUnitEnterable" />

                    
                
        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>