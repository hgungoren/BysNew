@page
@using Microsoft.AspNetCore.Mvc.Localization
@using ToksozBysNew.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using ToksozBysNew.Web.Pages.Specs
@using ToksozBysNew.Specs;
@using System.Globalization
@inject IHtmlLocalizer<ToksozBysNewResource> L
@model CreateModalModel
@{
    Layout = null;
}

<form data-ajaxForm="true" asp-page="/Specs/CreateModal" autocomplete="off">
    <abp-modal id="SpecCreateModal">
        <abp-modal-header title="@L["NewSpec"].Value"></abp-modal-header>

        <abp-modal-body>
                    
            <abp-input asp-for="Spec.SpecCode" />

            <abp-input asp-for="Spec.SpecName" />

                    
                
        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>