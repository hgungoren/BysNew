@page
@using Microsoft.AspNetCore.Mvc.Localization
@using ToksozBysNew.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using ToksozBysNew.Web.Pages.CustomerAddresses
@using ToksozBysNew.CustomerAddresses;
@using System.Globalization
@inject IHtmlLocalizer<ToksozBysNewResource> L
@model CreateModalModel
@{
    Layout = null;
}

<form data-ajaxForm="true" asp-page="/CustomerAddresses/CreateModal" autocomplete="off">
    <abp-modal id="CustomerAddressCreateModal">
        <abp-modal-header title="@L["NewCustomerAddress"].Value"></abp-modal-header>

        <abp-modal-body>
                    
            <abp-input asp-for="CustomerAddress.Address" />

                                <abp-select asp-for="CustomerAddress.DoctorId" asp-items="Model.DoctorLookupList" label="@L["Doctor"].Value"/>            <abp-select asp-for="CustomerAddress.BrickId" asp-items="Model.BrickLookupList" label="@L["Brick"].Value"/>            <abp-select asp-for="CustomerAddress.DistrictId" asp-items="Model.DistrictLookupList" label="@L["District"].Value"/>            <abp-select asp-for="CustomerAddress.CountryId" asp-items="Model.CountryLookupList" label="@L["Country"].Value"/>            <abp-select asp-for="CustomerAddress.ProvinceId" asp-items="Model.ProvinceLookupList" label="@L["Province"].Value"/>
                
        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>